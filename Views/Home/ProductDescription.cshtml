@model Product
@{
    var setcik = 0;
}

<section style="padding-top: 80px;">
    <div class="shop-main container mt-1 py-5 mb-5">
        <div class="row">
            <div class="col-lg-7">
                <div class="images d-flex">
                    <div class="left nomedia">
                        @foreach (var item in Model.Photos)
                        {
                            if (setcik == 0)
                            {
                                <div class="img selected-img">
                                    <img src="/img/productsPhoto/@item.PhotoName" alt="">
                                </div>
                            }
                            else
                            {
                                <div class="img">
                                    <img src="/img/productsPhoto/@item.PhotoName" alt="">
                                </div>
                            }
                            setcik++;
                        }
                    </div>
                    <div class="product-single__image-item " style="width: 700px; margin-right: 32px;">
                        <img src="" alt="">
                    </div>
                </div>
            </div>
            <div class="col-lg-5">
                <div class="d-flex justify-content-between mb-4 pb-md-2">
                    <div class="breadcrumb mb-0 d-none d-md-block flex-grow-1">
                        <a href="#" class="menu-link menu-link_us-s text-uppercase fw-500">Home</a>
                        <span class="breadcrumb-separator menu-link fw-500 ps-1 pe-1">/</span>
                        <a href="#" class="menu-link menu-link_us-s text-uppercase fw-500">The Shop</a>
                    </div><!-- /.breadcrumb -->

                </div>
                <h1 class="product-single__name">@Model.ProductName</h1>
                <div class="product-single__rating">
                    <div class="reviews-group d-flex">
                        @for (int i = 1; i < 6; i++)
                        {
                            if (Model.ProductAverageRating > i)
                            {
                                <svg xmlns="http://www.w3.org/2000/svg" width="11" height="11" fill="currentColor" class="bi bi-star-fill me-1" viewBox="0 0 16 16">
                                    <path class="text-warning"
                                          d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
                                </svg>
                            }
                            else if (Math.Ceiling((decimal)Model.ProductAverageRating) == i)
                            {
                                <svg xmlns="http://www.w3.org/2000/svg" width="11" height="11" fill="currentColor"
                                     class="bi bi-star-half me-1" viewBox="0 0 16 16">
                                    <path class="text-warning"
                                          d="M5.354 5.119 7.538.792A.52.52 0 0 1 8 .5c.183 0 .366.097.465.292l2.184 4.327 4.898.696A.54.54 0 0 1 16 6.32a.55.55 0 0 1-.17.445l-3.523 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256a.5.5 0 0 1-.146.05c-.342.06-.668-.254-.6-.642l.83-4.73L.173 6.765a.55.55 0 0 1-.172-.403.6.6 0 0 1 .085-.302.51.51 0 0 1 .37-.245zM8 12.027a.5.5 0 0 1 .232.056l3.686 1.894-.694-3.957a.56.56 0 0 1 .162-.505l2.907-2.77-4.052-.576a.53.53 0 0 1-.393-.288L8.001 2.223 8 2.226z" />
                                </svg>
                            }
                            else
                            {
                                <svg xmlns="http://www.w3.org/2000/svg" width="11" height="11" fill="currentColor"
                                     class="bi bi-star me-1" viewBox="0 0 16 16">
                                    <path class="text-warning"
                                          d="M2.866 14.85c-.078.444.36.791.746.593l4.39-2.256 4.389 2.256c.386.198.824-.149.746-.592l-.83-4.73 3.522-3.356c.33-.314.16-.888-.282-.95l-4.898-.696L8.465.792a.513.513 0 0 0-.927 0L5.354 5.12l-4.898.696c-.441.062-.612.636-.283.95l3.523 3.356-.83 4.73zm4.905-2.767-3.686 1.894.694-3.957a.56.56 0 0 0-.163-.505L1.71 6.745l4.052-.576a.53.53 0 0 0 .393-.288L8 2.223l1.847 3.658a.53.53 0 0 0 .393.288l4.052.575-2.906 2.77a.56.56 0 0 0-.163.506l.694 3.957-3.686-1.894a.5.5 0 0 0-.461 0z" />
                                </svg>
                            }
                        }
                    </div>
                    <span class="reviews-note text-secondary ms-1 ">@Model.ProductRatings reviews</span>
                </div>
                <div class="product-single__price">
                    <span class="current-price">$@Model.ProductPrice</span>
                </div>
                <div class="product-single__short-desc">
                    <p>
                        @Model.ProductAbout
                    </p>
                </div>
                <form name="addtocart-form" method="post" asp-controller="user" asp-action="AddToBasket" asp-route-id="@Model.ProductId">
                    <div class="product-single__swatches">
                        <div class="product-swatch text-swatches">
                            <label>Sizes</label>
                            <div class="swatch-list">
                                @foreach (Size size in ViewBag.Sizes)
                                {
                                    <input type="radio" name="BasketProductSizeId" id="@("swatch-"+size.SizeId)" value="@size.SizeId">
                                    <label class="swatch js-swatch" for="@("swatch-"+size.SizeId)">@size.SizeName</label>
                                }
                            </div>
                            @* <a href="#" class="sizeguide-link" data-bs-toggle="modal"
                            data-bs-target="#sizeGuide">
                            Size
                            Guide
                            </a> *@
                        </div>
                        <div class="product-swatch color-swatches">
                            @* <label>Color</label>
                            <div class="swatch-list">
                            <input type="radio" name="color" id="swatch-11">
                            <label class="swatch swatch-image js-swatch" for="swatch-11" aria-label="Red"
                            data-bs-toggle="tooltip" data-bs-placement="top" title=""
                            data-bs-original-title="Red">
                            <img loading="lazy"
                            src="https://uomo-html.flexkitux.com/images/products/swatch_0.jpg"
                            width="60" height="60" alt="">
                            </label>
                            <input type="radio" name="color" id="swatch-12" checked="">
                            <label class="swatch swatch-image js-swatch" for="swatch-12" aria-label="Blue"
                            data-bs-toggle="tooltip" data-bs-placement="top" title=""
                            data-bs-original-title="Blue">
                            <img loading="lazy"
                            src="https://uomo-html.flexkitux.com/images/products/swatch_1.jpg"
                            width="60" height="60" alt="">
                            </label>
                            <input type="radio" name="color" id="swatch-13">
                            <label class="swatch swatch-image js-swatch" for="swatch-13" aria-label="Black"
                            data-bs-toggle="tooltip" data-bs-placement="top" title=""
                            data-bs-original-title="Black">
                            <img loading="lazy"
                            src="https://uomo-html.flexkitux.com/images/products/swatch_2.jpg"
                            width="60" height="60" alt="">
                            </label>
                            </div> *@
                        </div>
                    </div>
                    <div class="product-single__addtocart">
                        <div class="qty-control position-relative qty-initialized">
                            <input type="number" name="BasketProductQuantity" class="visually-hidden" value="1" min="1" max="10">
